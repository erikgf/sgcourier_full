{{#registros}}
    <tr>
      <td class="td-opc text-center">
        <input type="checkbox"  {{#if_ bloqueado '==' '1'}}disabled{{/if_}} class="val_selected">
      </td>
      <td class="td-estado">
        <select  class="val_estado" {{#if_ descripcion_motivacion '!=' ''}}title="Motivo: {{descripcion_motivacion}}" {{/if_}} data-previous="{{estado}}" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} required name="txtestadopreliquidacion{{@index}}" style="width:100%;height:24px;">
          {{#../estados}}
            <option value="{{id}}" {{#if_ ../estado '==' id}}selected{{/if_}}>{{descripcion}}</option>
          {{/../estados}}
        </select>
      </td>
      <td>
        <input class="val_cliente" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} style="width:100%" autocomplete required name="txtclientepreliquidacion_{{@index}}" value="{{cliente}}"/>
      </td>
      <td>
        <input class="val_clienteinterno" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} style="width:100%" autocomplete required name="txtclienteinternopreliquidacion_{{@index}}" value="{{cliente_interno}}"/>
      </td>
      <td class="td-numeroguia">
        <input class="val_numeroguia" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} required name="txtnumeroguiapreliquidacion_{{@index}}" style="width:100%" value="{{documento_guia}}"/>
      </td>
      <td class="td-cantidad">
        <input class="val_cantidad" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} required name="txtcantidadpreliquidacion_{{@index}}" style="width:100%" type="number" value="{{cantidad}}"/>
      </td>
      <td class="td-tipopaquete">
        <select class="val_tipopaquete" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} required style="width:100%;height:24px;">
      		<option value="1" {{#if_ tipo_paquete '==' '1'}}selected{{/if_}}>PAQUETE</option>
      		<option value="2" {{#if_ tipo_paquete '==' '2'}}selected{{/if_}}>SOBRE</option>
      		<option value="3" {{#if_ tipo_paquete '==' '3'}}selected{{/if_}}>CAJA</option>
      		<option value="4" {{#if_ tipo_paquete '==' '4'}}selected{{/if_}}>MUEBLE</option>
      		<option value="5" {{#if_ tipo_paquete '==' '5'}}selected{{/if_}}>MUDANZA</option>
      		<option value="6" {{#if_ tipo_paquete '==' '6'}}selected{{/if_}}>PAQUETON</option>
      		<option value="7" {{#if_ tipo_paquete '==' '7'}}selected{{/if_}}>SACO</option>
      		<option value="8" {{#if_ tipo_paquete '==' '8'}}selected{{/if_}}>CARGA</option>
      		<option value="9" {{#if_ tipo_paquete '==' '9'}}selected{{/if_}}>OTRO</option>
        </select>
      </td>
      <td class="td-peso">
        <input class="val_peso" {{#if_ bloqueado '==' '1'}}disabled{{/if_}}  name="txtpesopreliquidacion_{{@index}}" style="width:100%" step="0.0001"  type="number" value="{{peso}}"/>
      </td>
      <td class="td-volumen">
        <input class="val_volumen" {{#if_ bloqueado '==' '1'}}disabled{{/if_}}  name="txtvolumenpreliquidacion_{{@index}}" style="width:100%" step="0.0001" type="number" value="{{volumen}}"/>
      </td>
      <td class="td-direccion">
        <input class="val_direccionentrega" {{#if_ bloqueado '==' '1'}}disabled{{/if_}}  name="txtdireccionpreliquidacion_{{@index}}" style="width:100%"  value="{{direccion_entrega}}"/>
      </td>
      <td class="td-ciudad">
        <input class="val_lugarentrega" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} autocomplete style="width:100%"  name="txtciudadpreliquidacion_{{@index}}"  value="{{lugar_entrega}}"/>
      </td>
      <td class="td-costounitario">
        <input  class="val_costounitario" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} step="0.001" required name="txtcostounitariopreliquidacion_{{@index}}"  style="width:100%" type="number" value="{{costo_unitario}}"/>
      </td>
      <td class="td-costosubtotal">
        <input  class="val_subtotal" {{#if_ bloqueado '==' '1'}}disabled{{/if_}} step="0.001"  required name="txtsubtotalpreliquidiacion_{{@index}}" style="width:100%" type="number" readonly value="{{subtotal}}"/>
      </td>
    </tr>
{{/registros}}
